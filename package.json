{
  "name": "tournadeck",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "_prettier": "prettier .",
    "formatCheck": "yarn run _prettier --check",
    "format": "yarn run _prettier --write",
    "lint": "next lint --max-warnings 0",
    "lintFix": "yarn run lint --fix",
    "test": "jest test",
    "testUnit": "yarn run test --testPathPattern=.*\\.spec\\.ts",
    "dc": "depcruise src test",
    "dcGraph": "depcruise -v -T dot src -x '(node_modules|pocketbase)' | dot -T svg | depcruise-wrap-stream-in-html > dependency-graph.html",
    "ci": "yarn run formatCheck && yarn run lint && yarn run dc && yarn run testUnit"
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "fp-ts": "2.16.1",
    "io-ts": "2.2.21",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "next": "14.0.4",
    "pino": "8.17.1",
    "pino-pretty": "10.3.0",
    "pocketbase": "0.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-cli-only": "0.2.2",
    "swr": "2.2.4",
    "type-fest": "4.8.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "10.4.16",
    "dependency-cruiser": "15.5.0",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-fp-ts": "0.3.2",
    "eslint-plugin-tailwindcss": "3.13.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "vite-node": "1.0.4"
  }
}
