{
  "name": "tournadeck",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "_prettier": "prettier .",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "next lint --max-warnings 0",
    "lintFix": "yarn lint --fix",
    "test": "jest",
    "testUnit": "yarn test --testPathPattern=.*\\.spec\\.ts",
    "dc": "depcruise src test",
    "dcGraph": "depcruise -v -T dot src -x '(node_modules|pocketbase)' | dot -T svg | depcruise-wrap-stream-in-html > dependency-graph.html",
    "ci": "yarn tsc && yarn formatCheck && yarn lint && yarn dc && yarn testUnit",
    "draftlolLink": "RUNNING_IN_CLI=true vite-node src/scripts/draftlolLink.ts",
    "parseGame": "RUNNING_IN_CLI=true vite-node src/scripts/parseGame.ts",
    "tailwindConfigViewer": "npx tailwind-config-viewer serve -p 3001"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "fp-ts": "2.16.9",
    "io-ts": "2.2.21",
    "ky": "1.7.2",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "next": "15.0.3",
    "pino": "9.5.0",
    "pino-pretty": "12.0.0",
    "pocketbase": "0.21.5",
    "puppeteer": "23.7.1",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-popper": "2.3.0",
    "server-cli-only": "0.3.2",
    "swr": "2.2.5",
    "type-fest": "4.26.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.9.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "10.4.20",
    "dependency-cruiser": "16.6.0",
    "eslint": "8.57.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-fp-ts": "0.3.2",
    "eslint-plugin-tailwindcss": "3.17.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "vite-node": "2.1.4"
  },
  "resolutions": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
