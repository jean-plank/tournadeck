{
  "name": "tournadeck",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "_prettier": "prettier .",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "next lint --max-warnings 0",
    "lintFix": "yarn lint --fix",
    "test": "jest",
    "testUnit": "yarn test --testPathPattern=.*\\.spec\\.ts",
    "dc": "depcruise src test",
    "dcGraph": "depcruise -v -T dot src -x '(node_modules|pocketbase)' | dot -T svg | depcruise-wrap-stream-in-html > dependency-graph.html",
    "tailwindConfigViewer": "npx tailwind-config-viewer serve -p 3001",
    "ci": "yarn tsc && yarn formatCheck && yarn lint && yarn dc && yarn testUnit"
  },
  "dependencies": {
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "fp-ts": "2.16.1",
    "io-ts": "2.2.21",
    "ky": "1.1.3",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "next": "14.0.4",
    "pino": "8.17.2",
    "pino-pretty": "10.3.1",
    "pocketbase": "0.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-cli-only": "0.2.2",
    "sharp": "0.33.1",
    "swr": "2.2.4",
    "type-fest": "4.9.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "autoprefixer": "10.4.16",
    "dependency-cruiser": "16.0.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-fp-ts": "0.3.2",
    "eslint-plugin-tailwindcss": "3.13.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
    "vite-node": "1.1.0"
  }
}
